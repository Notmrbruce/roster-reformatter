{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "buildCommand": "bash build.sh"
      }
    },
    { "src": "api/process_csv.py", "use": "@vercel/python@3.1.0" }
  ],
  "routes": [
    { "src": "/api/process_csv", "dest": "/api/process_csv.py" },
    { "src": "/(.*)", "dest": "/" }
  ],
  "env": {
    "PYTHONPATH": "api"
  },
  "build": {
    "env": {
      "PYTHON_VERSION": "3.9"
    }
  }
}